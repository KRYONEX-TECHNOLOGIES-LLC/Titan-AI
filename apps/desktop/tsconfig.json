{
  "$schema": "https://json.schemastore.org/tsconfig",
  "extends": "../../config/tsconfig/electron.json",
  "compilerOptions": {
    "outDir": "dist",
    "rootDir": "src",
    "noUnusedLocals": false,
    "noUnusedParameters": false,
    "exactOptionalPropertyTypes": false
  },
  "include": ["src/**/*.ts"],
  "exclude": ["node_modules", "dist", "out"]
}
