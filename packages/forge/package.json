{
  "name": "@titan/forge",
  "version": "0.1.0",
  "description": "Titan Forge â€” Knowledge distillation pipeline for training a custom LLM from elite model outputs",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./harvester-filter": {
      "import": "./dist/harvester-filter.js",
      "require": "./dist/harvester-filter.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup src/index.ts src/cli/export.ts src/cli/eval.ts src/cli/harvest.ts src/cli/harvest-continuous.ts src/cli/backup.ts --format esm,cjs",
    "dev": "tsup src/index.ts src/cli/export.ts src/cli/eval.ts src/cli/harvest.ts src/cli/harvest-continuous.ts src/cli/backup.ts --format esm,cjs --watch",
    "lint": "eslint src/",
    "typecheck": "tsc --noEmit",
    "clean": "rimraf dist",
    "test": "vitest run",
    "export": "node dist/cli/export.js",
    "eval": "node dist/cli/eval.js",
    "harvest": "node dist/cli/harvest.js",
    "harvest:continuous": "node dist/cli/harvest-continuous.js",
    "backup": "node dist/cli/backup.js"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.45.4",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@types/node": "^20.17.10",
    "rimraf": "^5.0.0",
    "tsup": "^8.3.5",
    "typescript": "^5.7.2",
    "vitest": "4.0.18"
  },
  "peerDependencies": {
    "typescript": "^5.0.0"
  },
  "keywords": [
    "distillation",
    "llm",
    "training",
    "titan",
    "forge"
  ],
  "author": "KRYONEX TECHNOLOGIES LLC",
  "license": "SEE LICENSE IN LICENSE"
}
